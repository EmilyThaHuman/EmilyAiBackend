    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "dist",
      "coverage",
      "*.test.js",
      "*.spec.js"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --experimental-specifier-resolution=node --es-module-specifier-resolution=node"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,yaml,yml",
    "watch": [
      "src/**/*.js",
      "config/**/*.js",
      "src/models",
      "config",
      "package.json",
      "jest.config.js"
    ],
    "exec": "node --inspect=9229 index.js"
  },
  "_moduleAliases": {
    "@app": "src/app.js",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@routes": "src/routes",
    "@services": "src/services",
    "@utils": "src/utils",
    "@config": "src/config",
    "@db": "src/db",
    "@middlewares": "src/middlewares",
    "@lib": "src/lib",
    "@public": "public",
    "@test": "src/__test__",
    "@logger": "src/config/logging"
  }
